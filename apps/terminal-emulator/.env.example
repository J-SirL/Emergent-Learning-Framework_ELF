# Terminal Emulator Configuration
# Copy this file to .env and fill in your values

# Development mode (load React from Vite dev server at localhost:5173)
DEV_MODE=true

# GitHub OAuth App Credentials
# Create an OAuth app at: https://github.com/settings/developers
# Set authorization callback URL to: http://localhost:8765/callback
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here

# Leaderboard Repository
# Create a public GitHub repository to store leaderboard.json
LEADERBOARD_REPO_OWNER=your_github_username
LEADERBOARD_REPO_NAME=terminal-emulator-leaderboard
